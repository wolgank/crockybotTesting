import discord
from discord.ext import commands

# Define los intents que tu bot necesitará
intents = discord.Intents.default()
intents.message_content = True  # Asegúrate de habilitar los intents necesarios

# Configura el prefijo de los comandos y los intents
client = commands.Bot(command_prefix=PREFIJO_COMANDOS, intents=intents)

# Tu código adicional aquí

client.run('TU_TOKEN_AQUÍ')
